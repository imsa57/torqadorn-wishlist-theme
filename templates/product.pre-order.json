ery-title {\n    margin: 0 0 4px 0;\n}\n\n.standard-delivery p {\n    margin: 0;\n}\n\n.prepaid-delivery, .cod-delivery {\n    padding-left: 0px;\n}\n\n.express-option-btn {\n    width: 100%;\n    background-color: #f9f9f9;\n    border: none;\n    border-radius: 6px;\n    padding: 12px 16px;\n    cursor: pointer;\n    transition: background-color 0.15s ease;\n}\n\n.express-option-btn:hover {\n    background-color: #e5e7eb;\n}\n\n.btn-content {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n}\n\n.left-content {\n    display: flex;\n    align-items: center;\n    gap: 12px;\n    font-size: 12px;\n    color: #1673B0;\n}\n\n.express-icon {\n    width: 20px;\n    height: 20px;\n}\n\n.chevron-icon {\n    width: 16px;\n    height: 16px;\n    transition: transform 0.2s ease;\n    filter: brightness(0) saturate(100%) invert(20%) sepia(0%) saturate(0%) hue-rotate(223deg) brightness(98%) contrast(94%);\n}\n\n.chevron-icon.open {\n    transform: rotate(180deg);\n}\n\n.express-details {\n    background-color: #F4F4F4;\n    border-radius: 4px;\n    padding: 12px 16px;\n}\n\n.express-details h3 {\n    color: #1673B0;\n    font-size: 12px;\n    font-weight: 400;\n    margin: 0 0 4px 0;\n}\n\n.price-text {\n    font-weight: 400;\n}\n\n.express-details p {\n    color: #6b7280;\n    font-size: 12px;\n    margin: 0;\n}\n</style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n    function addDaysSkippingWeekends(date, days) {\n        let result = new Date(date);\n        let addedDays = 0;\n        while (addedDays < days) {\n            result.setDate(result.getDate() + 1);\n            if (result.getDay() !== 0 && result.getDay() !== 6) {\n                addedDays++;\n            }\n        }\n        return result;\n    }\n\n    function formatDate(date) {\n        return date.toLocaleDateString('en-US', { \n            month: 'short', \n            day: 'numeric' \n        });\n    }\n\n    const today = new Date();\n\n    const prepaidDate = addDaysSkippingWeekends(today, 7);\n    document.getElementById('prepaid-delivery').textContent = \n        `${formatDate(prepaidDate)} (Prepaid Orders)`;\n\n    const codStartDate = addDaysSkippingWeekends(today, 8);\n    const codEndDate = addDaysSkippingWeekends(today, 9);\n    document.getElementById('cod-delivery').textContent = \n        `${formatDate(codStartDate)} - ${formatDate(codEndDate)} (COD Orders)`;\n\n    const expressDate = addDaysSkippingWeekends(today, 4);\n    document.getElementById('express-delivery').textContent = \n        `Expected by: ${formatDate(expressDate)}`;\n\n    const toggleBtn = document.getElementById('toggle-options');\n    const expressOptions = document.getElementById('express-options');\n    const chevron = document.querySelector('.chevron-icon');\n\n    toggleBtn.addEventListener('click', function() {\n        const isHidden = expressOptions.style.display === 'none';\n        expressOptions.style.display = isHidden ? 'block' : 'none';\n        chevron.classList.toggle('open');\n    });\n});\n</script>\n</head>\n</html>"
