        "custom_liquid_WD9Bnk": {
          "type": "custom_liquid",
          "disabled": true,
          "settings": {
            "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<style>\n.dlv-carousel-container {\n  width: 100%;\n  position: relative;\n  background: #f9f9f9;\n  border-radius: 8px;\n  overflow: hidden;\n}\n\n.dlv-carousel-track {\n  display: flex;\n  transition: transform 0.3s ease-in-out;\n  width: 300%;\n}\n\n.dlv-carousel-slide {\n  flex: 0 0 33.33%;\n  padding: 16px 20px;\n  min-width: 33.33%;\n}\n\n.dlv-slide-title {\n  font-size: 14px;\n  color: #333333;\n  margin: 0 0 8px;\n  font-weight: 400;\n  line-height: 1.4;\n}\n\n.dlv-slide-timeframe {\n  color: #666;\n}\n\n.dlv-slide-description {\n  font-size: 14px;\n  color: #848A96;\n  margin: 0;\n  line-height: 1.4;\n}\n\n.dlv-payment-icons {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n  margin-top: 8px;\n}\n\n.payment-icon {\n  width: 20px;\n  height: 20px;\n  object-fit: contain;\n}\n\n.dlv-carousel-indicators {\n  position: absolute;\n  top: 50%;\n  right: 20px;\n  transform: translateY(-50%);\n  display: flex;\n  flex-direction: row;\n  gap: 6px;\n  z-index: 2;\n}\n\n.dlv-indicator {\n  width: 15px;\n  height: 2px;\n  background: #ccc;\n  cursor: pointer;\n  transition: background-color 0.2s ease;\n  border-radius: 1px;\n}\n\n.dlv-indicator.active {\n  background: #000;\n}\n\n@media (max-width: 768px) {\n  .payment-icon {\n    width: 28px;\n    height: 28px;\n  }\n  .dlv-carousel-slide {\n    padding: 12px 16px;\n  }\n}\n</style>\n</head>\n<body>\n<div class=\"dlv-carousel-container\">\n  <div class=\"dlv-carousel-track\">\n    <div class=\"dlv-carousel-slide\">\n      <h3 class=\"dlv-slide-title\">Secure Payments</h3>\n      <div class=\"dlv-payment-icons\">\n        <img src=\"https://cdn.shopify.com/s/files/1/0526/4811/6407/files/visa_1.svg?v=1720857724\" alt=\"Visa\" class=\"payment-icon\" />\n        <img src=\"https://cdn.shopify.com/s/files/1/0526/4811/6407/files/mastercard_symbol.svg?v=1720857821\" alt=\"Mastercard\" class=\"payment-icon\" />\n        <img src=\"https://cdn.shopify.com/s/files/1/0526/4811/6407/files/amex-svg.svg?v=1720857811\" alt=\"American Express\" class=\"payment-icon\" />\n        <img src=\"https://cdn.shopify.com/s/files/1/0526/4811/6407/files/UPI-Logo-vector.svg?v=1720858027\" alt=\"UPI\" class=\"payment-icon\" />\n        <img src=\"https://cdn.shopify.com/s/files/1/0526/4811/6407/files/Cash_on_Delivery_21055eb1-5198-480f-9b7c-a9b7320d3488.svg?v=1739127719\" alt=\"Cash on Delivery\" class=\"payment-icon\" />\n      </div>\n    </div>\n    <div class=\"dlv-carousel-slide\">\n      <h3 class=\"dlv-slide-title\">Easy Customisations</h3>\n      <p class=\"dlv-slide-description\">Custom fits available. Just ask.</p>\n    </div>\n    <div class=\"dlv-carousel-slide\">\n      <h3 class=\"dlv-slide-title\">Personal Stylist</h3>\n      <p class=\"dlv-slide-description\">Fashion advice on speed dial. DM us!</p>\n    </div>\n  </div>\n  \n  <div class=\"dlv-carousel-indicators\">\n    <div class=\"dlv-indicator active\" data-index=\"0\"></div>\n    <div class=\"dlv-indicator\" data-index=\"1\"></div>\n    <div class=\"dlv-indicator\" data-index=\"2\"></div>\n  </div>\n</div>\n\n<script>\n(function() {\n  const track = document.querySelector('.dlv-carousel-track');\n  const container = document.querySelector('.dlv-carousel-container');\n  const indicators = document.querySelectorAll('.dlv-indicator');\n  const slides = document.querySelectorAll('.dlv-carousel-slide');\n\n  let currentSlide = 0;\n  let startX, moveX;\n  let isTransitioning = false;\n\n  function goToSlide(index) {\n    if (isTransitioning) return;\n    \n    let targetIndex = index;\n    if (index < 0) targetIndex = slides.length - 1;\n    if (index >= slides.length) targetIndex = 0;\n    \n    isTransitioning = true;\n    track.style.transition = 'transform 0.3s ease-in-out';\n    track.style.transform = `translateX(-${targetIndex * 33.33}%)`;\n    \n    indicators.forEach((indicator, i) => {\n      indicator.classList.toggle('active', i === targetIndex);\n    });\n    \n    currentSlide = targetIndex;\n    \n    setTimeout(() => {\n      isTransitioning = false;\n    }, 300);\n  }\n\n  function handleSwipe() {\n    if (!moveX) return;\n    \n    const diff = startX - moveX;\n    const threshold = container.offsetWidth / 4;\n    \n    if (Math.abs(diff) > threshold) {\n      goToSlide(diff > 0 ? currentSlide + 1 : currentSlide - 1);\n    } else {\n      goToSlide(currentSlide);\n    }\n  }\n\n  container.addEventListener('touchstart', (e) => {\n    if (isTransitioning) return;\n    startX = e.touches[0].clientX;\n    moveX = startX;\n    track.style.transition = 'none';\n  }, { passive: true });\n\n  container.addEventListener('touchmove', (e) => {\n    if (isTransitioning) return;\n    moveX = e.touches[0].clientX;\n    const diff = startX - moveX;\n    const offset = (currentSlide * 33.33) + (diff / container.offsetWidth * 33.33);\n    track.style.transform = `translateX(-${offset}%)`;\n  }, { passive: true });\n\n  container.addEventListener('touchend', () => {\n    handleSwipe();\n    moveX = null;\n  });\n\n  indicators.forEach((indicator, i) => {\n    indicator.addEventListener('click', () => goToSlide(i));\n  });\n\n  function autoPlay() {\n    if (!document.hidden && !isTransitioning) {\n      goToSlide(currentSlide + 1);\n    }\n  }\n\n  const autoPlayInterval = setInterval(autoPlay, 5000);\n\n  document.addEventListener('visibilitychange', () => {\n    if (document.hidden) {\n      clearInterval(autoPlayInterval);\n    }\n  });\n})();\n</script>\n</body>\n</html>"
          }
