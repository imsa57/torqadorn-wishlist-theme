d black;\n  background: transparent;\n  padding: 8px 20px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  white-space: nowrap;\n  margin-left: auto;\nborder-radius: 4px;\n}\n\n.eaw-learn-more-btn:hover {\n  background: #f5f5f5;\n}\n\n.eaw-popup-modal {\n  display: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0,0,0,0.5);\n  z-index: 2147483645;\n  align-items: center;\n  justify-content: center;\n  overflow-y: hidden;\n}\n\n.eaw-popup-content {\n  position: fixed;\n  background-color: white;\n  padding: 20px;\n  width: 90%;\n  max-width: 600px;\n  border-radius: 4px;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  z-index: 2147483646;\n  max-height: 90vh;\n  overflow-y: auto;\n}\n\n.eaw-close-popup {\n  position: absolute;\n  right: 10px;\n  top: 10px;\n  font-size: 24px;\n  cursor: pointer;\n  width: 30px;\n  height: 30px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 50%;\n  z-index: 2147483647;\n  background-color: white;\n}\n\n.eaw-close-popup:hover {\n  background-color: #f5f5f5;\n}\n\n.eaw-popup-images {\n  display: flex;\n  justify-content: space-around;\n  margin-bottom: 20px;\n  gap: 20px;\n}\n\n.eaw-popup-img {\n  width: 45%;\n  max-width: 250px;\n  height: auto;\n  object-fit: cover;\n}\n\n.eaw-popup-text {\n  text-align: center;\n  padding: 0 20px;\n}\n\n@media (max-width: 768px) {\n  .eaw-widget-content {\n    padding: 10px 12px;\n  }\n  \n  .eaw-promo-text {\n    flex-direction: column;\n    align-items: flex-start;\n    gap: 12px;\n  }\n\n  .eaw-learn-more-btn {\n    margin-left: 0;\n  }\n  \n  .eaw-popup-content {\n    width: 95%;\n    padding: 15px;\n  }\n  \n  .eaw-popup-images {\n    flex-direction: column;\n    align-items: center;\n  }\n  \n  .eaw-popup-img {\n    width: 100%;\n    margin-bottom: 10px;\n  }\n}\n</style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n