eaw-popup-img {\n    width: 100%;\n    margin-bottom: 10px;\n  }\n}\n</style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  let scrollPosition = 0;\n\n  function eawLockScroll() {\n    scrollPosition = window.pageYOffset;\n    document.body.classList.add('eaw-scroll-lock');\n    document.body.style.top = `-${scrollPosition}px`;\n  }\n\n  function eawUnlockScroll() {\n    document.body.classList.remove('eaw-scroll-lock');\n    document.body.style.top = '';\n    window.scrollTo(0, scrollPosition);\n  }\n\n  window.eawOpenPopup = function() {\n    const popup = document.getElementById('eaw-promoPopup');\n    popup.style.display = 'flex';\n    eawLockScroll();\n  }\n\n  window.eawClosePopup = function() {\n    const popup = document.getElementById('eaw-promoPopup');\n    popup.style.display = 'none';\n    eawUnlockScroll();\n  }\n\n  const popup = document.getElementById('eaw-promoPopup');\n  popup.addEventListener('click', function(event) {\n    if (event.target === popup) {\n      eawClosePopup();\n    }\n  });\n\n  const popupContent = document.querySelector('.eaw-popup-content');\n  popupContent.addEventListener('wheel', function(event) {\n    event.stopPropagation();\n  });\n\n  let eawBagsLeft = 2;\n  const eawStartTime = new Date().getTime();\n  \n  function eawUpdateCounter() {\n    const currentTime = new Date().getTime();\n    const minutesPassed = Math.floor((currentTime - eawStartTime) / (30 * 60 * 1000));\n    eawBagsLeft = Math.max(2 - (minutesPassed * 2), 0);\n    \n    document.getElementById('eaw-bags-left').textContent = eawBagsLeft;\n    \n    if (eawBagsLeft > 0) {\n      setTimeout(eawUpdateCounter, 60000);\n    }\n  }\n  \n  eawUpdateCounter();\n});\n</script>"
