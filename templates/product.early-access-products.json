            "custom_liquid": "<!DOCTYPE html>\n<html>\n<head>\n    <style>\n        .countdown-container {\n            font-family: Futura, Arial, sans-serif;\n            text-align: center;\n            padding: 5px;\n            max-width: 100%;\n        }\n\n        .timer-section {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            gap: 5px;\n            flex-wrap: wrap;\n            margin: 5px 0;\n        }\n\n        .timer-container {\n            display: flex;\n            align-items: center;\n            gap: 15px;\n        }\n\n        .timer-block {\n            display: flex;\n            align-items: center;\n            gap: 5px;\n        }\n\n        .timer-number {\n            font-size: 24px;\n        }\n\n        .timer-label {\n            font-size: 14px;\n        }\n\n        @media (max-width: 600px) {\n            .timer-section {\n                flex-direction: column;\n                gap: 10px;\n            }\n\n            .timer-container {\n                gap: 10px;\n            }\n\n            .timer-number {\n                font-size: 20px;\n            }\n\n            .timer-label {\n                font-size: 12px;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"countdown-container\">\n        <div class=\"timer-section\">\n            <div>Black Friday Event ends in</div>\n            <div class=\"timer-container\">\n                <div class=\"timer-block\">\n                    <div id=\"hours\" class=\"timer-number\">00</div>\n                    <div class=\"timer-label\">Hours</div>\n                </div>\n                <div class=\"timer-block\">\n                    <div id=\"minutes\" class=\"timer-number\">00</div>\n                    <div class=\"timer-label\">Minutes</div>\n                </div>\n                <div class=\"timer-block\">\n                    <div id=\"seconds\" class=\"timer-number\">00</div>\n                    <div class=\"timer-label\">Seconds</div>\n                </div>\n            </div>\n        </div>\n        <div>\n            30% off sitewide | Use code: BLACKFRIDAY30\n        </div>\n    </div>\n\n    <script>\n        function startCountdown() {\n            const endTime = new Date('2024-11-29T06:30:00Z').getTime();\n\n            function updateTimer() {\n                const now = new Date().getTime();\n                const timeLeft = endTime - now;\n\n                if (timeLeft < 0) {\n                    document.querySelectorAll('.timer-number').forEach(el => el.textContent = '00');\n                    clearInterval(timerInterval);\n                    return;\n                }\n\n                const hours = Math.floor((timeLeft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\n                const minutes = Math.floor((timeLeft % (1000 * 60 * 60)) / (1000 * 60));\n                const seconds = Math.floor((timeLeft % (1000 * 60)) / 1000);\n\n                document.getElementById('hours').textContent = hours.toString().padStart(2, '0');\n                document.getElementById('minutes').textContent = minutes.toString().padStart(2, '0');\n                document.getElementById('seconds').textContent = seconds.toString().padStart(2, '0');\n            }\n\n            updateTimer();\n            const timerInterval = setInterval(updateTimer, 1000);\n            window.addEventListener('unload', () => clearInterval(timerInterval));\n        }\n\n        document.addEventListener('DOMContentLoaded', startCountdown);\n    </script>\n</body>\n</html>"
