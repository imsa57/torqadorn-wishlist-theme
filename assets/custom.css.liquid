/* Custom Popup */
.main-product__block-size_popup.open_popup {
    display: block;
}
.m-header__wrapper.header_behind{
  z-index: 0;
}
.main-product__block-size_popup {
    position: fixed;
    display: none;
    transition: opacity 1s;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: #00000040;
    width: 100%;
    height: 100%;
    z-index: 9999998 !important;
    margin: 0 !important;
}
.popup-main-container {
    padding: inherit;
    position: fixed;
    top: 19%;
    left: 0px;
    right: 0px;
    margin: 0px auto;
    bottom: 0;
    z-index: 99999999;
    width: 100%;
}
 .popup-main-container .popup-content-container {
    display: block;
    justify-content: center;
    width: 100%;
    max-width: 980px !important;
    overflow: hidden;
    position: relative;
    margin: 0 auto;
    background: #fff;
    padding: 0;
   height: 660px;
       overflow-y: scroll;
}
.popup-content-container .left_box_content .line_item_properties {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 10px;
}
  header__wrapper.header_behind {
    z-index: 0;
}
.popup-content-container .left_box_content .line_item_properties p.line-item-property__field {
    display: flex;
    flex-direction: column;
    gap: 5px;
}
.popup-content-container .exit_btn {
    position: absolute;
    z-index: 999;
    right: 0;
}
.popup-content-container .exit_btn span {
    position: absolute;
    top: 7px;
    right: 20px;
    background: #000;
    color: #fff !important;
    padding: 5px 9px;
    border-radius: 50%;
    cursor: pointer;
    left: auto;
    z-index: 999;
    line-height: normal;
}
  .popup-main-container .right_box_image img {
    object-fit: cover;
    width: 100%;
    object-position: center center;
    display: flex;
}
.left_box_content {
    width: 50%;
}
.popup-main-container .right_box_image {
    width: 50%;
    position: relative;
}
.client_information {
    margin-top: 30px;
    display: block;
    padding-bottom: 30px;
}
.left_box_container {
    width: 100%;
