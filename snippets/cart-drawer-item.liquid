    %}
    <foxkit-incart-upsell data-collections="{{ collections | remove_last: ',' }}" data-product-title="{{ item.product.title }}" data-product-id="{{ item.product_id }}"></foxkit-incart-upsell>
  {% endcomment %}
</div>